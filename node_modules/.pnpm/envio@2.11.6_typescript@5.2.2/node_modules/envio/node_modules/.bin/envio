#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rafi/Desktop/transfers/node_modules/.pnpm/envio@2.11.6_typescript@5.2.2/node_modules/envio/node_modules:/Users/rafi/Desktop/transfers/node_modules/.pnpm/envio@2.11.6_typescript@5.2.2/node_modules:/Users/rafi/Desktop/transfers/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rafi/Desktop/transfers/node_modules/.pnpm/envio@2.11.6_typescript@5.2.2/node_modules/envio/node_modules:/Users/rafi/Desktop/transfers/node_modules/.pnpm/envio@2.11.6_typescript@5.2.2/node_modules:/Users/rafi/Desktop/transfers/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
