#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rafi/Desktop/transfers/node_modules/.pnpm/rescript@11.1.3/node_modules/rescript/node_modules:/Users/rafi/Desktop/transfers/node_modules/.pnpm/rescript@11.1.3/node_modules:/Users/rafi/Desktop/transfers/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rafi/Desktop/transfers/node_modules/.pnpm/rescript@11.1.3/node_modules/rescript/node_modules:/Users/rafi/Desktop/transfers/node_modules/.pnpm/rescript@11.1.3/node_modules:/Users/rafi/Desktop/transfers/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../rescript/rescript" "$@"
else
  exec node  "$basedir/../../../rescript/rescript" "$@"
fi
